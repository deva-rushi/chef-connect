<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane maxHeight="400.0" maxWidth="400.0" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HomePageController">
   <center>
      <VBox alignment="CENTER" spacing="20">
      	<Button fx:id="gotoThirdScreenButton" onAction="#handleGoToThird" text="Go to Third Screen" />
      	<Button fx:id="logoutButton" onAction="#handleLogout" text="Logout" />
      </VBox>
   </center>
   <top>
   	<Label fx:id="welcomeLabel" text="Welcome" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
