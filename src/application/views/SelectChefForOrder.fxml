<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.SelectChefForOrderController">
    <ComboBox fx:id="sortComboBox" prefWidth="150.0" />
    <ListView fx:id="chefsListView" prefHeight="200.0" prefWidth="200.0" />
    <Button mnemonicParsing="false" onAction="#handleSelectChef" text="Select Chef" />
    <Button mnemonicParsing="false" onAction="#handleBack" text="Back" />
</VBox>